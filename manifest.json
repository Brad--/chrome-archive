{
    "manifest_version": 2,
    "name": "Archive",
    "version": "0.0.1",
    "description": "A chrome tab management extension",
    "short_name": "Archive",
    "permissions": ["<all_urls>", "declarativeContent", "storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["background.css"],
            "js": ["background.js"]
        }
    ],
    "browser_action": {
        "default_title": "Does a thing",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png"
        }
    }
}