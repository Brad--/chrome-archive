{
    "manifest_version": 2,
    "name": "Archive",
    "version": "0.0.2",
    "description": "A chrome tab management extension",
    "short_name": "Archive",
    "permissions": ["<all_urls>", "declarativeContent", "storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["archive.css"],
            "js": ["archive.js"]
        }
    ],
    "browser_action": {
        "default_title": "Does a thing",
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icons/leek32.ico"
        }
    }
}